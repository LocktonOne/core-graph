{
  "name": "core-graph",
  "license": "MIT",
  "scripts": {
    "compile": "graph codegen && graph build",
    "create-local": "graph create --node $GRAPH_NODE_ENDPOINT <core-graph>",
    "remove-local": "graph remove --node $GRAPH_NODE_ENDPOINT <core-graph>",
    "deploy-local": "npm run compile && graph deploy --node $GRAPH_NODE_ENDPOINT --ipfs $IPFS_ENDPOINT <core-graph>",
    "update-config": "node scripts/config-parser.js"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.32.0",
    "@graphprotocol/graph-ts": "^0.27.0",
    "dotenv": "^16.0.3",
    "js-yaml": "^4.1.0",
    "node-vault": "^0.9.22"
  }
}
